{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Source/**.cs"
          ],
          "exclude": [
            "**/GoogleVR/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "Api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "Api/**.yml",
          "Api/index.md"
        ]
      },
      {
        "files": [
          "Articles/**.md"
        ]
      },
      {
        "files": [
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "Apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": ["docFxGlobalMetadata"],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}