{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Source/**.cs"
          ],
          "exclude": [
            "**/GoogleVR/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "files": [
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": ["docFxGlobalMetadata"],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}